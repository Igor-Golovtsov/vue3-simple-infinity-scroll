    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.3",
    "vue": "^3.0.5"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "vue3",
    "vue 3",
    "vue3 infinity scroll",
    "vue3 infinity loader",
    "infinity loader",
    "infinity loading",
    "infinity scroll"
