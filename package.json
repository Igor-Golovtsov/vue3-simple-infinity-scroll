{
  "name": "vue3-simple-infinity-scroll",
  "version": "0.0.1",
  "description": "Vue3 simple infinity scroll.",
  "author": "Igor Golovtsov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Igor-Golovtsov/vue3-simple-infinity-scroll.git"
  },
